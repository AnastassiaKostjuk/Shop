@model ShopTARgv24.Models.RealEstate.RealEstateDetailsViewModel

<h1 class="text-center">Details</h1>

<div>
    <h4>Spaceship</h4>
    <hr />
    <dl class="row">
        <div class="form-group row">
            <label asp-for="Id" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.Id)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="Area" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.Area)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="Location" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.Location)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="RoomNr" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.RoomNr)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="BuildingType" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.BuildingType)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.CreatedAt)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="ModifiedAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.ModifiedAt)
        </div>
      
    </dl>
    <a asp-action="Index" asp-controller="realestate" class="btn btn-secondary m-1">Back</a>
</div>